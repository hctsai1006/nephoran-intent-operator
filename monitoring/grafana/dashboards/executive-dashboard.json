    {
      "datasource": "Prometheus",
      "description": "Key business metrics summary table",
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "displayMode": "auto"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Status"
            },
            "properties": [
              {
                "id": "custom.displayMode",
                "value": "color-background"
              },
              {
                "id": "mappings",
                "value": [
                  {
                    "options": {
                      "pattern": "^Good$",
                      "result": {
                        "color": "green",
                        "index": 0
                      }
                    },
                    "type": "regex"
                  },
                  {
                    "options": {
                      "pattern": "^Warning$",
                      "result": {
                        "color": "yellow",
                        "index": 1
                      }
                    },
                    "type": "regex"
                  },
                  {
                    "options": {
                      "pattern": "^Critical$",
                      "result": {
                        "color": "red",
                        "index": 2
                      }
                    },
                    "type": "regex"
                  }
                ]
              }
            ]
          }
        ]
      },
