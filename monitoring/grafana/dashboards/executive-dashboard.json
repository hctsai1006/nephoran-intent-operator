          "instant": true,
          "refId": "B"
        },
        {
          "expr": "label_replace(nephoran_customer_satisfaction_score, \"Metric\", \"Customer Satisfaction\", \"\", \"\")",
          "format": "table",
          "instant": true,
          "refId": "C"
        },
        {
          "expr": "label_replace(nephoran:monthly_cost_projection, \"Metric\", \"Monthly OpEx\", \"\", \"\")",
          "format": "table",
          "instant": true,
          "refId": "D"
        }
      ],
