        {
          "expr": "((nephoran_automation_savings_usd_total) / nephoran_operational_costs_usd_total) * 100",
          "legendFormat": "Automation ROI",
          "refId": "B"
        },
        {
          "expr": "((nephoran_efficiency_savings_usd_total) / nephoran_operational_costs_usd_total) * 100",
          "legendFormat": "Efficiency ROI",
          "refId": "C"
        }
      ],
      "title": "Return on Investment Analysis",
      "type": "timeseries"
    },
