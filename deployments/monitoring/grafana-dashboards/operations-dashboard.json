        "id": 3,
        "options": {
          "legend": {
            "calcs": [],
            "displayMode": "list",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "single",
            "sort": "none"
          }
        },
        "targets": [
          {
            "expr": "100 * (1 - rate(nephoran_intent_failures_total[5m]) / rate(nephoran_intent_requests_total[5m]))",
            "legendFormat": "Availability",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(nephoran_intent_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "Latency P95",
            "refId": "B"
          },
          {
            "expr": "rate(nephoran_intent_processed_total[1m]) * 60",
            "legendFormat": "Throughput",
            "refId": "C"
          }
        ],
        "title": "Real-time SLA Metrics",
        "type": "timeseries"
      },
